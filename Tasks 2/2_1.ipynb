def f():
    string = input()
    current, maxlen = '', ''
    for i in range(len(string)):
        if string[i] not in current:
            current += string[i]
        else:
            if len(current) > len(maxlen):
                maxlen, current = current, string[i]
    print(maxlen)


if __name__ == "__main__":
    f()
